{
  "name": "patternlab-node-cli",
  "description": "Command-line interface (CLI) for the patternlab-node core.",
  "version": "0.0.1-alpha.4",
  "bin": {
    "patternlab": "bin/patternlab.js"
  },
  "author": {
    "name": "Raphael Okon"
  },
  "private": true,
  "dependencies": {
    "archiver": "1.3.0",
    "browser-sync": "2.18.11",
    "bs-html-injector": "3.0.3",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "execa": "0.6.3",
    "fs-promise": "2.0.3",
    "glob": "7.1.2",
    "has-yarn": "1.0.0",
    "inquirer": "2.0.0",
    "lodash": "4.17.4",
    "ora": "1.2.0",
    "path-exists": "3.0.0",
    "patternlab-node": "2.9.3",
    "sanitize-filename": "1.6.1"
  },
  "devDependencies": {
    "edition-node": "0.0.4",
    "eslint": "3.19.0",
    "proxyquire": "1.8.0",
    "starterkit-mustache-base": "3.0.1",
    "tap": "10.3.2"
  },
  "files": [
    "bin"
  ],
  "keywords": [
    "Pattern Lab",
    "Atomic Web Design",
    "Node",
    "JavaScript"
  ],
  "scripts": {
    "lint": "eslint ./{bin,test}",
    "test": "tap './test/*.test.js' --reporter spec --timeout=120",
    "pretest": "yarn run lint && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pattern-lab/patternlab-node-cli.git"
  },
  "bugs": "https://github.com/pattern-lab/patternlab-node-cli/issues",
